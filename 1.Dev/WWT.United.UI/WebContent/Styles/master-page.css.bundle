    <file>/WebContent/Styles/topBarMainNav.css</file>
    <file>/WebContent/Styles/topBarSideNav.css</file>
    <file>/WebContent/Styles/topBarHelpCenter.css</file>
    <file>/WebContent/Styles/newsgatorNgStylesModified.less</file>
